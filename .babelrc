{
  "env":{
    "node": {
      "presets": [["env", {
        "modules": false,
        "targets": {
          "node": "current"
        }
      }]],
      "plugins": [
        "syntax-dynamic-import"
      ]
    },
    "web": {
      "presets": [["env", {
        "modules": false,
        "targets": {
          "browsers": ["last 2 versions", "safari >= 7"]
        }
      }]],
      "plugins": [
        "transform-object-assign",
        "transform-vue-jsx",
      ]
    }
  },
  "plugins": [
    "transform-object-rest-spread",
    "syntax-dynamic-import",
    "transform-runtime"
  ],
  "comments":false
}
